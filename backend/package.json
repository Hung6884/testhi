{
  "name": "dat-backend",
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "description": "",
  "main": "./src/app/main/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run setupEnv && cross-env NODE_ENV=development nodemon src/index.js --ext js --watch",
    "electron:dev": "npm run setupEnv && cross-env NODE_ENV=development electron .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "swagger-autogen": "node src/swagger.js",
    "migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate:run": "cross-env NODE_ENV=development npx sequelize-cli db:migrate --name",
    "migrate:make": "cd ./src/database && cross-env NODE_ENV=development npx --workspaces=false sequelizemm --file ./sequelize.js",
    "migrate:undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo --name",
    "migrate:reset": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "seed:make": "cross-env NODE_ENV=development npx sequelize-cli seed:generate --name",
    "seed:run": "cross-env NODE_ENV=development npx sequelize-cli db:seed --seed",
    "seed:run-all": "cross-env NODE_ENV=development npx sequelize-cli db:seed:all",
    "seed:undo": "cross-env NODE_ENV=development npx sequelize-cli db:seed:undo --seed",
    "seed:undo-all": "cross-env NODE_ENV=development npx sequelize-cli db:seed:undo:all",
    "electron:build:win": "npm run setupEnv && cross-env NODE_ENV=production electron-builder --dir false --publish never --win",
    "electron:build:mac": "npm run setupEnv && cross-env NODE_ENV=production electron-builder --dir false --publish never --mac",
    "electron:build:linux": "npm run setupEnv && cross-env NODE_ENV=production electron-builder --dir false --publish never --linux",
    "rebuild": "electron-rebuild -f -w bcryptjs",
    "postinstall": "npm run rebuild && electron-builder install-app-deps && npm run setupEnv",
    "package": "npm run setupEnv && cross-env NODE_ENV=production electron-builder --dir false --publish never",
    "setupEnv": "node ./scripts/setupEnv.js",
    "format": "prettier --write .",
    "gen:models": "node scripts/generate-models.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**",
      "**/node_modules/**"
    ]
  },
  "node-module-alias": {
    "@": "src"
  },
  "dependencies": {
    "@babel/core": "7.24.9",
    "@babel/preset-env": "7.24.8",
    "@bqy/node-module-alias": "1.0.1",
    "axios": "1.7.2",
    "babel-plugin-module-resolver": "5.0.2",
    "bcryptjs": "2.4.3",
    "bluebird": "3.7.2",
    "cookie": "1.0.1",
    "cors": "2.8.5",
    "cross-env": "7.0.0",
    "crypto": "1.0.1",
    "date-fns": "4.1.0",
    "dotenv": "10.0.0",
    "electron-log": "5.2.0",
    "express": "4.17.1",
    "express-correlation-id": "2.0.1",
    "express-rate-limit": "5.0.0",
    "finalhandler": "1.3.1",
    "form-data": "4.0.1",
    "hash-sum": "2.0.0",
    "helmet": "4.1.0",
    "http-status": "1.7.4",
    "https": "1.0.0",
    "joi": "17.13.3",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "micromatch": "4.0.8",
    "module-alias": "^2.2.3",
    "moment": "2.24.0",
    "mongodb": "6.12.0",
    "morgan": "1.9.1",
    "multer": "1.4.5-lts.1",
    "node-mocks-http": "1.8.0",
    "node-red": "4.0.5",
    "node-red-contrib-play-audio": "2.5.0",
    "node-red-contrib-web-worldmap": "5.0.4",
    "node-red-dashboard": "3.6.5",
    "node-red-node-base64": "0.3.0",
    "node-red-node-geofence": "0.3.4",
    "node-red-node-random": "0.4.1",
    "node-red-node-smooth": "0.1.2",
    "nodemailer": "6.3.1",
    "nodemon": "3.1.4",
    "npm": "10.9.0",
    "pg": "8.14.1",
    "pg-hstore": "2.3.4",
    "querystring": "0.2.1",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.37.3",
    "sequelize-cli": "6.6.2",
    "sequelizemm": "1.3.0",
    "socket.io": "4.8.0",
    "swagger-autogen": "2.23.7",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.3",
    "tedious": "18.6.1",
    "tough-cookie": "5.0.0",
    "url": "0.11.4",
    "uuid": "11.0.3",
    "validator": "13.0.0",
    "winston": "3.2.1",
    "xlsx": "0.18.5",
    "xss-clean": "0.1.1"
  },
  "devDependencies": {
    "electron": "29.4.6",
    "electron-builder": "25.1.8",
    "electron-rebuild": "3.2.9",
    "eslint": "8.39.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-security": "1.4.0",
    "mysql2": "^3.15.0",
    "prettier": "3.3.0",
    "sequelize-auto": "^0.8.8"
  }
}
